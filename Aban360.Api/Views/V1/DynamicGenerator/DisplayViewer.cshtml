@using Stimulsoft.Report.Mvc;
@using Stimulsoft.Report.Web;

@{
    ViewBag.Title = "مشاهده";
}

@Html.StiNetCoreViewer(new StiNetCoreViewerOptions()
{
    Actions =
    {
        GetReport = "display",
        ViewerEvent="event"
    },
    Localization = "~/AppData/DynamicReport/fa.xml",
    Appearance =
    {
        ShowTooltipsHelp = false,
        ShowDialogsHelp = false,
        ShowTooltips=false,
        CustomCss=@"~/AppData/Stylesheets/sti-custom.css"
    },
    Theme = StiViewerTheme.Office2022WhitePurple,
    Toolbar =
    {
        ShowAboutButton=false,
    }    
})

<link href="~/AppData/Stylesheets/sti-custom.css" rel="stylesheet" />